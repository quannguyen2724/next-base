{
  "extends": [
    "next",
    "eslint:recommended"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "no-empty": "off",
    "no-undef": "off",
    "react/display-name": "off",
    "jsx-a11y/alt-text": "off",
    "import/no-anonymous-default-export": "off"
  },
  "overrides": [
    {
      "files": [
        "*.d.ts"
      ],
      "rules": {
        "no-unused-vars": "off"
      }
    }
  ]
}